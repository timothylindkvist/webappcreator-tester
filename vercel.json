{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "nodejs22.x"
    },
    "api/*.ts": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}